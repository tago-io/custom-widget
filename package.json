{
  "name": "@tago/custom-widget",
  "description": "TagoIO Toolkit to build your own widgets",
  "version": "1.0.5",
  "main": "./dist/custom-widget.js",
  "author": "Tago LLC",
  "license": "Apache-2.0",
  "repository": "tago-io/custom-widget",
  "scripts": {
    "build:scripts": "webpack && MINIMIZE=true webpack",
    "build:css": "lessc ./src/css/custom-widget.less ./dist/custom-widget.css",
    "build:cssmin": "cleancss -o ./dist/custom-widget.min.css ./dist/custom-widget.css",
    "build": "npm run build:scripts && npm run build:css && npm run build:cssmin",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.28.0",
    "clean-css-cli": "4.3.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3",
    "less": "3.11.1",
    "prettier": "2.0.4",
    "shortid": "2.2.15",
    "ts-loader": "7.0.0",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "dependencies": {}
}
